{
  "name": "reactjs-boilerplate",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:e2e": "cp -n example.env.local .env.local && next build",
    "start": "next start",
    "lint": "eslint .",
    "prepare": "is-ci || husky",
    "release": "release-it",
    "generate:resource": "hygen generate resource",
    "postgenerate:resource": "npm run lint -- --fix",
    "lint:fix": "eslint . --fix",
    "generate:field": "hygen generate field",
    "postgenerate:field": "npm run lint -- --fix",
    "api:generate": "orval",
    "api:generate:file": "orval --input ./api-json.json",
    "api:generate:watch": "orval --watch",
    "sb": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@faker-js/faker": "9.8.0",
    "@hookform/resolvers": "5.2.1",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-oauth/google": "0.12.2",
    "@remixicon/react": "^4.9.0",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "5.87.1",
    "@tanstack/react-query-devtools": "5.87.1",
    "@types/node": "24.3.1",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "accept-language": "3.0.20",
    "autoprefixer": "^10.4.24",
    "clsx": "^2.1.1",
    "date-fns": "3.6.0",
    "dotenv": "16.5.0",
    "eslint": "9.35.0",
    "eslint-config-next": "15.5.2",
    "html2canvas": "^1.4.1",
    "i18next": "25.5.2",
    "i18next-browser-languagedetector": "8.2.0",
    "i18next-resources-to-backend": "1.2.1",
    "js-cookie": "3.0.5",
    "jspdf": "^4.2.0",
    "nanoid": "5.1.5",
    "next": "15.5.9",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-dropzone": "14.3.8",
    "react-hook-form": "7.62.0",
    "react-i18next": "15.7.3",
    "react-toastify": "11.0.5",
    "react-virtuoso": "4.14.0",
    "recharts": "^3.7.0",
    "tailwind-merge": "^3.4.1",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.9.2",
    "yup": "1.7.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.1",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@playwright/test": "1.55.0",
    "@release-it/conventional-changelog": "10.0.1",
    "@storybook/addon-docs": "9.1.5",
    "@storybook/addon-links": "9.1.5",
    "@storybook/addon-onboarding": "9.1.5",
    "@storybook/nextjs": "9.1.5",
    "@types/js-cookie": "3.0.6",
    "@typescript-eslint/eslint-plugin": "8.42.0",
    "@typescript-eslint/parser": "8.42.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-storybook": "9.1.5",
    "husky": "9.1.7",
    "hygen": "6.2.11",
    "is-ci": "4.1.0",
    "orval": "^8.4.0",
    "prettier": "3.6.2",
    "release-it": "19.0.4",
    "storybook": "9.1.5"
  },
  "release-it": {
    "$schema": "https://unpkg.com/release-it/schema/release-it.json",
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "perf",
              "section": "Performance Improvements"
            },
            {
              "type": "revert",
              "section": "Reverts"
            },
            {
              "type": "docs",
              "section": "Documentation"
            },
            {
              "type": "refactor",
              "section": "Code Refactoring"
            },
            {
              "type": "test",
              "section": "Tests"
            },
            {
              "type": "ci",
              "section": "Continuous Integration"
            }
          ]
        }
      }
    }
  },
  "overrides": {
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9"
  }
}
